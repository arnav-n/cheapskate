{
"manifest_version": 3,
"name": "Cheapskate",
"description": "User-curated games lists",
"version": "1.0",
"icons": {
    "16": "images/logo.png",
    "48": "images/icon48.png"
  },
"action": {
    "default_popup": "window.html"
    },
"content_scripts": [
        {
            "matches": ["http://*/*"],
            "css": ["./background.css"]
        }
    ]
}